{
  "name": "clide-plugin-command-menu",
  "version": "1.0.0",
  "description": "A plugin for Clide-JS CLIs to auto-prompt the user for missing required subcommands.",
  "license": "MIT",
  "type": "module",
  "author": "ryan goree (https://github.com/ryangoree)",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryangoree/clide-js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "NODE_ENV=development tesm-node example/cli.ts",
    "test": "vitest --reporter=verbose",
    "test:ci": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "clide-js": "^0.2.0"
  },
  "dependencies": {
    "cfonts": "^3.2.0",
    "colors": "^1.4.0",
    "tesm-node": "^1.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "clide-js": "^0.2.0",
    "ts-node": "^10.9.2",
    "tsconfig": "*",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "clide-js",
    "prompt",
    "interactive"
  ]
}
